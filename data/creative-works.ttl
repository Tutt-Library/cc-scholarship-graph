    schema:url "https://doi.org/10.1002/2017GL074312" ;
    cite:authorString "Colton Lynner, Megan L. Anderson, Daniel E. Portner, Susan L. Beck, Hersh Gilbert." ;
    cite:month "JUL 16" .

